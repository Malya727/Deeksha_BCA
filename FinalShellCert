#!/bin/bash

# === CONFIGURATION VARIABLES ===
KeyStorePath="/path/to/your_cert.pem"      # Public certificate
KeyFile="/path/to/your_private_key.key"    # Private key
FlowId="your_flow_id_here"

# === AUTHENTICATE WITH ANAPLAN ===
